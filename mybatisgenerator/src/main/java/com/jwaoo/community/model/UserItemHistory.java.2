package com.jwaoo.community.model;

import java.util.Date;

public class UserItemHistory {
    private Long id;

    private Long uid;

    private Long itemId;

    private Long targetId;

    private Integer num;

    private Integer beforNum;

    private Integer reason;

    private Long reasonParam;

    private String reasonDetail;

    private Date createTime;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Long getUid() {
        return uid;
    }

    public void setUid(Long uid) {
        this.uid = uid;
    }

    public Long getItemId() {
        return itemId;
    }

    public void setItemId(Long itemId) {
        this.itemId = itemId;
    }

    public Long getTargetId() {
        return targetId;
    }

    public void setTargetId(Long targetId) {
        this.targetId = targetId;
    }

    public Integer getNum() {
        return num;
    }

    public void setNum(Integer num) {
        this.num = num;
    }

    public Integer getBeforNum() {
        return beforNum;
    }

    public void setBeforNum(Integer beforNum) {
        this.beforNum = beforNum;
    }

    public Integer getReason() {
        return reason;
    }

    public void setReason(Integer reason) {
        this.reason = reason;
    }

    public Long getReasonParam() {
        return reasonParam;
    }

    public void setReasonParam(Long reasonParam) {
        this.reasonParam = reasonParam;
    }

    public String getReasonDetail() {
        return reasonDetail;
    }

    public void setReasonDetail(String reasonDetail) {
        this.reasonDetail = reasonDetail;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }
}